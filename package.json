{
  "name": "ddd-app",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": "^16.20.0"
  },
  "scripts": {
    "data:seed:dev": "NODE_ENV=dev ts-node ./src/infra/data/seed.ts",
    "data:seed:test": "NODE_ENV=test ts-node ./src/infra/data/seed.ts",
    "postinstall": "",
    "clean:build": "rm -rf ./.build",
    "clean:all": "rm -rf ./.build && rm -rf ./node_modules && rm package-lock.json",
    "clean:install": "rm -rf ./.build && rm -rf ./node_modules && npm install",
    "build": "npm run clean:build && tsc --build tsconfig.json",
    "postbuild": "cp serverless.yml .build",
    "start": "npm run local:start:docker",
    "local:start:dev": "npm run lint && NODE_ENV=dev serverless offline",
    "local:start:test": "NODE_ENV=test serverless offline",
    "local:start:docker": "rm -rf .build && NODE_ENV=docker serverless offline  --host 0.0.0.0  --useDocker",
    "docker:compose:all": "docker compose -f \"docker-compose.yml\" up -d --build",
    "docker:compose:daemon": "docker compose -f \"docker-compose-daemon-only.yml\" up -d --build",
    "docker:stop": "docker-compose stop",
    "docker:restart": "docker compose -f \"docker-compose.yml\" down && npm run docker:compose",
    "docker:clean": "docker system prune -a",
    "deployold": "cd .build && serverless deploy --verbose && cd ..",
    "deploy": "serverless deploy --verbose",
    "build-deploy": "npm run build && npm run deploy",
    "pretest": "npm run lint",
    "test": "npm run test:ci",
    "test:dev": "NODE_ENV=dev npx jest --config=jest.config.test.js --detectOpenHandles --verbose",
    "test:ci": "NODE_ENV=ci npx jest --config=jest.config.test.js --detectOpenHandles --verbose",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dd-trace": "^4.8.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mock-aws-s3": "^4.0.2",
    "mongoose": "^7.5.0",
    "nock": "^13.3.2",
    "npm": "^9.8.1",
    "redis": "^4.6.7",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^8.0.2",
    "@types/aws-lambda": "^8.10.51",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-jest": "^29.0.3",
    "esbuild": "^0.17.19",
    "esbuild-node-externals": "^1.8.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-no-async-foreach": "^0.1.1",
    "jest": "^29.5.0",
    "serverless-bundle": "^6.0.0",
    "serverless-esbuild": "^1.45.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-datadog": "^5.36.0",
    "ts-node": "^10.9.1",
    "typescript": "4.4.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web2solutions/ddd-app.git"
  },
